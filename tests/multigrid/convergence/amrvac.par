 &filelist
        base_filename='test'
    	convert_type='vtuBCCmpi'
        autoconvert=T
 /

 &savelist
        dtsave_dat=0.05d0
 /

 &stoplist
        time_max=1.0d0
 /

 &methodlist
        time_integrator=  'threestep'
        flux_scheme = 20*'tvdlf'
        limiter= 20*'koren'
 /

 &boundlist
        typeboundary_min1='cont'
        typeboundary_max1='cont'
        typeboundary_min2='cont'
        typeboundary_max2='cont'
        typeboundary_min3='cont'
        typeboundary_max3='cont'
 /

 &meshlist
        refine_max_level=3
        refine_threshold=20*1.0
        domain_nx1=32
        domain_nx2=32
        domain_nx3=32
        xprobmin1=-0.5d0
        xprobmax1=0.5d0
        xprobmin2=-0.5d0
        xprobmax2=0.5d0
        xprobmin3=-0.5d0
        xprobmax3=0.5d0
 /

 &paramlist
       courantpar=0.8d0
 /

 &rho_list
        rho_v = 0.0,0.0,0.0
 /

